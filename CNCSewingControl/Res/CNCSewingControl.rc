                MENUITEM "水绿色样式(&A)",                   ID_VIEW_APPLOOK_OFF_2007_AQUA
            END
        END
    END
    POPUP "功能模块(&F)"
    BEGIN
        MENUITEM "缝制信息(&I)",                    ID_SEWINGINFO_SET
        MENUITEM "花样选择(&C)",                    ID_SEWFILE_SET
